{
    "name": "template.nuxt",
    "version": "1.0.0",
    "description": "Nuxt.js Template project",
    "author": "Novichkov <novichkov.alexey@gmail.com>",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env BASE_URL=finolog.awe nuxt --config-file ./projects/$PR/$PR.nuxt.config.js",
        "generate": "nuxt generate --config-file ./projects/$PR/$PR.nuxt.config.js && cp -R ./projects/$PR/static/* build/$PR/",
        "create": "ncp projects/blank projects/$PR"
    },
    "dependencies": {
        "@nuxtjs/google-analytics": "2.2.2",
        "@nuxtjs/style-resources": "1.0.0",
        "core-js": "3.4.8",
        "cross-env": "6.0.3",
        "js-cookie": "2.2.1",
        "nuxt": "2.10.2",
        "setka": "0.2.0",
        "stylus": "0.54.7",
        "stylus-loader": "3.0.2",
        "vue-lazyload": "1.3.3"
    },
    "devDependencies": {
        "@bazzite/nuxt-optimized-images": "0.3.0",
        "babel-eslint": "10.0.3",
        "eslint": "6.7.2",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-loader": "3.0.3",
        "eslint-plugin-import": "2.19.1",
        "eslint-plugin-vue": "6.0.1",
        "imagemin-mozjpeg": "8.0.0",
        "imagemin-pngquant": "8.0.0",
        "imagemin-svgo": "7.0.0",
        "ncp": "2.0.0",
        "regenerator-runtime": "0.13.3",
        "sharp": "0.23.4",
        "vue-svg-loader": "0.15.0"
    }
}
