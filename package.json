{
    "name": "template.nuxt",
    "version": "1.0.0",
    "description": "Nuxt.js Template project",
    "author": "Novichkov <novichkov.alexey@gmail.com>",
    "private": true,
    "scripts": {
        "blank:dev": "cross-env BASE_URL=finolog.awe nuxt --config-file ./projects/blank/blank.nuxt.config.js",
        "www:dev": "cross-env BASE_URL=finolog.awe nuxt --config-file ./projects/www/www.nuxt.config.js",
        "www:generate": "nuxt generate --config-file ./projects/www/www.nuxt.config.js && cp -R ./projects/www/static/* build/www/",
        "ico:dev": "nuxt --config-file ./projects/ico/ico.nuxt.config.js",
        "ico:generate": "nuxt generate --config-file ./projects/ico/ico.nuxt.config.js && cp -R ./projects/ico/static/* build/ico/",
        "onboarding:dev": "nuxt --config-file ./projects/onboarding/onboarding.nuxt.config.js",
        "onboarding:generate": "nuxt generate --config-file ./projects/onboarding/onboarding.nuxt.config.js && cp -R ./projects/onboarding/static/* build/onboarding/"
    },
    "dependencies": {
        "@bazzite/nuxt-optimized-images": "0.2.2",
        "@nuxtjs/google-analytics": "2.2.2",
        "@nuxtjs/style-resources": "1.0.0",
        "core-js": "3.4.8",
        "cross-env": "6.0.3",
        "imagemin-mozjpeg": "8.0.0",
        "imagemin-pngquant": "8.0.0",
        "imagemin-svgo": "7.0.0",
        "js-cookie": "2.2.1",
        "nuxt": "2.10.2",
        "responsive-loader": "1.2.0",
        "setka": "0.2.0",
        "sharp": "0.23.4",
        "stylus": "0.54.7",
        "stylus-loader": "3.0.2",
        "vue-lazyload": "1.3.3",
        "vue-svg-loader": "0.15.0"
    },
    "devDependencies": {
        "babel-eslint": "10.0.3",
        "eslint": "6.7.2",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-import": "2.19.1",
        "eslint-plugin-vue": "6.0.1",
        "regenerator-runtime": "0.13.3"
    }
}
